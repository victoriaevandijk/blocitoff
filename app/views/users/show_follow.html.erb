<div class="container">
  <div class="row">
    <div class='col-sm-12'>
      <h1><%= @user.name %></h1>
      <span><%= link_to "view my profile", @user %></span>
      <br><br>
      <div class="row">
        <% @users.each do |user| %>
        <ul>
            <%= link_to gravatar_for(user), user %>
            <%= link_to user.name, user %>
        </ul>
         <% end %>
      </div>
    </div>
  </div>
</div>